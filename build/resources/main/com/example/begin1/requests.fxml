<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="567.0" prefWidth="762.0" style="-fx-background-color: #D7E0E6;"
            stylesheets="@../../../models/requests.css" xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FriendshipRequestController">
    <children>
        <Button fx:id="acceptButton" layoutX="14.0" layoutY="420.0" mnemonicParsing="false"
                onAction="#handleAcceptButton" prefHeight="26.0" prefWidth="142.0" text="Accept"/>
        <Button fx:id="deleteButton" layoutX="527.0" layoutY="420.0" mnemonicParsing="false"
                onAction="#handleDeleteButton" prefHeight="26.0" prefWidth="142.0" text="Delete"/>
        <Button fx:id="closeButton" layoutX="605.0" layoutY="517.0" mnemonicParsing="false"
                onAction="#handleCloseButton" prefHeight="26.0" prefWidth="142.0" text="Close" textFill="#ea0707"/>
        <TableView fx:id="requestTable" prefHeight="419.0" prefWidth="685.0">
            <columns>
                <TableColumn fx:id="usernameColumn" prefWidth="340.8000122308731" text="From"/>
                <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="342.3999938964844" text="Status"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
